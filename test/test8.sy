// 测试程序8: 一维数组与函数参数
int add_arr(int n, int a[]) {
    int i, s;
    i = 0;
    s = 0;
    while (i < n) {
        s = s + a[i];
        i = i + 1;
    }
    return s;
}

int main() {
    const int SID1 = 23050721;
