// 测试程序12: 二维数组与逻辑组合
int sum2d(int r, int c, int m[2][3]) {
    int i, j, s;
    i = 0;
    s = 0;
    while (i < r) {
        j = 0;
        while (j < c) {
            s = s + m[i][j];
            j = j + 1;
        }
        i = i + 1;
    }
    return s;
}

int main() {
    const int SID1 = 23050721;
    const int SID2 = 23080508;
    putint(SID1); putch(32); putint(SID2); putch(10);

    int mat[2][3] = {{1, 2, 3}, {4, 5, 6}};
    int total;
    total = sum2d(2, 3, mat);
    if (total == 21 || (total > 0 && total < 30)) {
        return total;
    } else {
        return -1;
    }
}

